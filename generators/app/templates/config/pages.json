{
  "application-name": "<%= projectName %>",
  "application-description": "<%= projectDescription %>"
  "default-root": "home",
  "not-found-tag": "web-404",
  "not-authorized-tag": "web-403",
  "pages": [
    {
      "tag": "web-home",
      "source": "/src/pages/web-home/web-home.html",
      "not-included-in-router": false,
      "not-included-in-links": false,
      "link-label": "Home",
      "route": "home"
    },
    {
      "tag": "web-404",
      "source": "/src/pages/web-404/web-404.html",
      "not-included-in-router": true,
      "not-included-in-links": true
    },
    {
      "tag": "web-403",
      "source": "/src/pages/web-403/web-403.html",
      "not-included-in-router": true,
      "not-included-in-links": true
    }
  ]
}