{
  "entrypoint": "index.html",
  "shell": "src/<%= projectNameSlugged %>-app.html",
  "fragments": [
    "src/pages/web-home/web-home.html",
    "src/pages/web-404/web-404.html",
    "src/pages/web-403/web-403.html"
  ],
  "sourceGlobs": [
   "src/**/*",
   "images/**/*",
   "bower.json",
   "config/**/*",
   "fonts/**/*"
  ],
  "includeDependencies": [
    "bower_components/app-storage/app-indexeddb-mirror/app-indexeddb-mirror-worker.js",
    "bower_components/app-storage/app-indexeddb-mirror/common-worker-scope.js",
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.min.js"
  ]
}